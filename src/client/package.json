  "name": "@thinktool/client",
  "version": "3.14.1",
  "description": "Client code for Thinktool, shared between web and desktop client.",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/c2d7fa/thinktool.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@types/prosemirror-model": "^1.13.0",
    "@types/prosemirror-state": "^1.2.6",
    "@types/prosemirror-view": "^1.17.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@johv/immutable-extras": "^1.1.1",
    "@johv/miscjs": "^1.6.0",
    "@thinktool/search": "^1.1.0",
    "@thinktool/shared": "^2.0.0",
    "immutable": "^4.0.0-rc.12",
    "prosemirror-model": "^1.14.0",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.18.3"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "scripts": {
    "build-style": "mkdir -p dist && node_modules/.bin/sass app.scss dist/app.css",
    "check": "node_modules/.bin/tsc",
    "build": "npm run build-style && node_modules/.bin/babel src/ -D --extensions .ts,.tsx --out-dir dist && tsc",
    "watch": "npm run build && node_modules/.bin/babel -w --verbose src/ -D --extensions .ts,.tsx --out-dir dist",
    "test": "jest --roots tests --roots src",
    "test-watch": "jest --watchAll --roots tests --roots src"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/main.js",
  "types": "dist/main.d.ts"
}
